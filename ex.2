pragma solidity 0.7.0;                         // Version pragma
pragma experimental ABIEncoderV2;               // Experimental pragma
contract Calc {   

function add(uint n1,uint n2) public pure returns(uint) {
    return n1 + n2;
}

function sub(uint n1, uint n2) public pure returns(uint) {
    return n1 - n2;
}

function mul(uint n1,uint n2) public pure returns(uint) {
    return n1 * n2;
}
function div(uint numerator, uint denominator) public pure returns(uint quotient, uint remainder) {
        quotient  = numerator / denominator;
        remainder = numerator - denominator * quotient;
    }
}


